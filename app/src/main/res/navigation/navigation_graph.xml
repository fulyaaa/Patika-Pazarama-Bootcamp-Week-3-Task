<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        tools:layout="@layout/fragment_first"
        android:name="com.example.week_3_assignment_fulyaaa.FirstFragment"
        android:label="FirstFragment" >
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/secondFragment" />
        <action
            android:id="@+id/action_firstFragment_to_secondFragment2"
            app:destination="@id/secondFragment2" />
        <action
            android:id="@+id/action_firstFragment_to_secondFragment3"
            app:destination="@id/secondFragment3" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment"
        tools:layout="@layout/fragment_second"
        android:name="com.example.week_3_assignment_fulyaaa.SecondFragment"
        android:label="SecondFragment" >
        <action
            android:id="@+id/action_secondFragment_to_firstFragment"
            app:destination="@id/firstFragment" />
        <action
            android:id="@+id/action_secondFragment_to_thirdFragment"
            app:destination="@id/thirdFragment" />
    </fragment>
    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.week_3_assignment_fulyaaa.ThirdFragment"
        android:label="fragment_third"
        tools:layout="@layout/fragment_third" />
    <fragment
        android:id="@+id/secondFragment2"
        android:name="com.example.week_3_assignment_fulyaaa.SecondFragment2"
        android:label="fragment_second2"
        tools:layout="@layout/fragment_second2" >
        <action
            android:id="@+id/action_secondFragment2_to_thirdFragment"
            app:destination="@id/thirdFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment3"
        android:name="com.example.week_3_assignment_fulyaaa.SecondFragment3"
        android:label="fragment_second3"
        tools:layout="@layout/fragment_second3" >
        <action
            android:id="@+id/action_secondFragment3_to_thirdFragment"
            app:destination="@id/thirdFragment" />
    </fragment>
</navigation>